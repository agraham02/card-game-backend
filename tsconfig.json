{
    "compilerOptions": {
        /* Visit https://aka.ms/tsconfig to read more about this file */

        "outDir": "./dist",
        "rootDir": "./src",
        "target": "ES6",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true /* Skip type checking all .d.ts files. */
    },
    "include": ["src/**/*.ts"]
}
